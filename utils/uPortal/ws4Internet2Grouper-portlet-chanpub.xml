<!DOCTYPE channel-definition SYSTEM "channelDefinition.dtd">
<channel-definition>
	<title>ESCO Web Service for Grouper Portlet</title>
	<name>ESCO Web Service for Grouper Portlet</name>
	<fname>ws4Internet2Grouper</fname>
	<desc>ESCO Web Service for Grouper Portlet</desc>
	<type>Portlet</type>
	<class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
	<timeout>20000</timeout>
	<hasedit>N</hasedit>
	<hashelp>N</hashelp>
	<hasabout>N</hasabout>
	<secure>N</secure>
	<locale>en_US</locale>
	<categories>
		<category>uPortal</category>
	</categories>
	<groups>
		<group>Tous les groupes de personnes</group>
	</groups>
	<parameters>

		<parameter>
			<name>portletDefinitionId</name>
			<value>ws4Internet2Grouper.ws4Internet2Grouper</value>
			<description>
				Identifies the portlet deployed within the portlet
				container
			</description>
			<ovrd>N</ovrd>
		</parameter>
	</parameters>
</channel-definition>
